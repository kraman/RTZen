<project default="compile" basedir="../../../../../../../../../../..">

	<property file="zen-build-bootstrap.properties"/>
	<property file="zen-build.properties"/>
	
	<property name="jrate.dir" value="edu/uci/ece/doc/tools/ant/taskdefs/compilers"/>
	
	<target name="compile">
		<javac
			srcdir="${zen.anttaskdir}"
			destdir="${zen.classdir}"
			includes="${jrate.dir}/jRate.java">
		</javac>
	</target>

	<target name="javadoc">
		<javadoc
			sourcefiles="${zen.anttaskdir}/${jrate.dir}/jRate.java"
			destdir="${zen.anttaskdir}/${jrate.dir}/docs"
			author="true"/>
	</target>
	
</project>

